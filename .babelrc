{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "chrome": "67",
          "edge": "17",
          "firefox": "60",
          "ie": "11",
          "safari": "11.1",
        },
      },
    ],
    [ "@babel/preset-react" ],
  ],
  "plugins": [
    [ "react-hot-loader/babel" ],
    [ "@babel/plugin-proposal-object-rest-spread" ],
    [ "@babel/plugin-proposal-class-properties", { "loose": true } ],
    [ "@babel/plugin-proposal-optional-chaining", { "loose": true } ],
  ]
}
